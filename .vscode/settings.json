{
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.config": "./deno.json",
  "deno.importMap": "./import_map.json",
  "deno.suggest.imports.hosts": {
    "https://deno.land": true,
    "https://esm.sh": false
  }
}